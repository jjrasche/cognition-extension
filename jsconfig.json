{
  "compilerOptions": {
    "checkJs": true,
    "target": "ES2022",
    "moduleResolution": "node",
    "module": "ESNext",
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noImplicitThis": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "maxNodeModuleJsDepth": 0,
    "noEmit": true
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "typeAcquisition": {
    "include": [
      "chrome"
    ]
  },
  "include": [
    "*.js",
    "*.d.ts"
, "types/type.d.ts"  ]
}